modules = ["nodejs-20", "web", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[userenv]

[userenv.shared]
NEXT_PUBLIC_SUPABASE_URL = "https://placeholder.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY = "placeholder"
NEXT_PUBLIC_APP_URL = "https://placeholder.replit.dev"
NEXT_PUBLIC_SITE_URL = "https://placeholder.replit.dev"
SUPABASE_SERVICE_ROLE_KEY = "placeholder"

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "start", "--", "-p", "5000", "-H", "0.0.0.0"]
build = ["npm", "run", "build"]
